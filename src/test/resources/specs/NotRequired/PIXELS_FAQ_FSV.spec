@objects
    FAQPageTitle               xpath  //section[@id='faq']//h2
    FirstFAQTitle              xpath  (//section[@id='faq']//div[@class='accordion-tab']/div)[1]
    FirstFAQCollapsedSign      xpath  (//section[@id='faq']//div[@class='right-button']/span)[1]
    FirstFAQSummaryText        xpath  //section[@id='faq']//div[@class='accordion-content show-content']
   
    
    
= Blog Listing =
    @on Desktop
        FAQPageTitle:
            inside screen  194 to 204px left
           
        FirstFAQTitle:
            inside screen 193 to 206px left
            below FAQPageTitle 36 to 48px         
            
        FirstFAQCollapsedSign:
            inside screen 891 to 904px left
               
                       
        
             
        
    @on tablet
        FAQPageTitle:
            inside screen  16 to 28px left
           
        FirstFAQTitle:
            inside screen 11 to 22px left
            below FAQPageTitle 	26 to 42px  
            
            
        FirstFAQCollapsedSign:
            inside screen 893 to 908px left
            
            
       
            
       
    @on Mobile
        FAQPageTitle:
            inside screen  10 to 22px left
            
            
        FirstFAQTitle:
            inside screen 10 to 22px left
            below FAQPageTitle 23 to 35px       
            
        FirstFAQCollapsedSign:
            inside screen 466 to 482px left
           
            
       
            
    
             